#include <iostream>
using namespace std;

int a,b,c,d;
void greater_no()
    {
        if (a>=b && a>=c && a>=d)
        {cout<<a;}
        else if(b>=a && b>=c && b>=d)
        {cout<<b;}
        else if(c>=a && c>=b && c>=d)
        {cout<<c;}
        else
        {cout<<d;}
    };
int main()
{
    cin>>a;
    cin>>b;
    cin>>c;
    cin>>d;
    greater_no(); 
    return 0;
}
