#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a,b;
    cin>>a>>b;
    int *p =&a;
    int *q = &b;
int temp=a;
    a = *p + *q;
    b = abs(temp - *q );
    cout<<a;
    cout<<"\n";
    cout<<b;
    return 0;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
