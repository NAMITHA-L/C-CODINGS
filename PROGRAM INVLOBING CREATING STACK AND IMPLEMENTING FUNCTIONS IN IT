#include<iostream>
#include<stack>
using namespace std;   
int main()
{
    stack <int> stack1;
    stack1.push(6);
    stack1.push(150);
    stack1.push(70);
    stack1.push(40);
    stack1.push(0);
    stack1.push(200);
    stack1.push(180);
    stack1.push(0);
    stack1.push(90);
    stack1.push(10);
   
    
    cout<<"the elements are:"<<endl;
    while(!stack1.empty())
    {
        cout<<stack1.top()<<"\t"<<endl;
        stack1.pop();
    }
    
    cout<<stack1.size();
    cout<<"the current element :"<<endl;
    cout<<stack1.top();
    stack1.pop();
    

    
    cout<<"after deleting elements to the stack"<<endl;
    while(!stack1.empty())
    {
        cout<<stack1.top()<<endl;
        stack1.pop();
    }
    
    
    
    if (stack1.empty())
    {
        cout<<"the stack is empty"<<endl;
    }
    else
    {
        cout<<"The stack is not empty and the size is:"<<stack1.size()<<endl;
    }
    
    return 0;
}


