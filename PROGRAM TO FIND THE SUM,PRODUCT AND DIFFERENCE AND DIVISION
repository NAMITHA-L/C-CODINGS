#include <iostream>
using namespace std;

int main() {
    int A, B;
    cout << "Enter two integers A and B: ";
    cin >> A >> B;
    cout << A + B << " " << A * B << " ";
    if (A > B) {
        cout << A - B << " " << A / B << endl;
    } else {
        cout << B - A << " " << B / A << endl;
    }

    return 0;
}
