class flower:
    def __init__(self,price,color):
        self.price=price
        self.color=color
    def display(self):
        print(f"FLOWER PRICE IS :{self.price} AND COLOR IS:{self.color}")
rose=flower(price=100,color="red")
sunflower= flower( price =150 , color = "yellow")

rose.display()
sunflower.display()
