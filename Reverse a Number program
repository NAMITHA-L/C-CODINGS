#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;

    cout << "Enter the number of elements:" << endl;
    cin >> n;

    vector<int> arr(n);  // Use vector for dynamic size

    cout << "Enter the numbers:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "Reversed order of elements:" << endl;
    for (int i = n - 1; i >= 0; i--) {
        cout << arr[i] << " ";
    }
    
    cout << endl;
    return 0;
}
