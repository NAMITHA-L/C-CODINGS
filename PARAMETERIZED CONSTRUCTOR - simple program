#include<iostream>
using namespace std;
class Test 
{   private:
    int a;
    public:
    Test(int s)//parameter is given so it is a parameterized  constructor
    {
        a = s;
        cout<<"The constructor is called"<<endl;
    }
};
int main()
{
    Test t1(5);// object initialization with parameter.
    cout<<"\nConstructor End"<<endl;
    return 0;
}
